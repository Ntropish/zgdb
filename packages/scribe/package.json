{
  "name": "@tsmk/scribe",
  "version": "1.0.0",
  "description": "Console Logger for tsmk",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "concurrently \"npm run test:jest\" \"npm run test:types\"",
    "test:jest": "jest --verbose",
    "test:types": "tsd -f src/index.test-d.ts",
    "run:simple": "node dist/examples/simple.js",
    "example:simple": "run-s build run:simple",
    "run:chaining": "node dist/examples/chaining.js",
    "example:chaining": "run-s build run:chaining",
    "run:low-level": "node dist/examples/low-level.js",
    "example:low-level": "run-s build run:low-level",
    "run:direct": "node dist/examples/direct.js",
    "example:direct": "run-s build run:direct"
  },
  "dependencies": {
    "@tsmk/kernel": "workspace:*",
    "@tsmk/log": "workspace:*",
    "@tsmk/tint": "workspace:*"
  },
  "devDependencies": {
    "@tsmk/schema": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.12.12",
    "@types/node-fetch": "^2.6.11",
    "@types/supertest": "^6.0.2",
    "axios": "^1.10.0",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "run-s": "^0.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "tsd": "^0.31.0",
    "typescript": "^5.4.5"
  },
  "tsd": {
    "directory": "src"
  }
}
